#include<iostream>
#include<string>

using namespace std;

class bucky{
	private :
		string name;                                 /// good practice to keep variable private 
		public:
			void Setname (string x){
				name=x;
			}
			string Getname(){               /// remember the type of function and what are you trying to return
			                                   ///using variable in the class
				return name;
			}
};

int main(){
	bucky bo;
	bo.Setname("fahim");
	cout<<bo.Getname();
	
}
