#include<iostream>
#include<string>

using namespace std;

class bucky{
	private :
		string name;
		public:
			void Setname (string x){
				name=x;
			}
			string Getname(){               /// remember the type of function and what are you trying to return
				return name;
			}
};

int main(){
	bucky bo;
	bo.Setname("fahim");
	cout<<bo.Getname();
	
}
